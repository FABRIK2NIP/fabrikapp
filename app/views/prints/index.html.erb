<h1>Prints#index</h1>
<p>Find me in app/views/prints/index.html.erb</p>

<% if !flash[:notice].blank?%>
	<div class="alert-info">
		<%= flash[:notice]%>
	<div>
<% end %>
<br />
<%= link_to "New Print", new_print_path, class: "btn btn-primary"%>
<br / >
<br/ >
<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>Name</th>
			<th>Download Link </th>
			<th> Matériau </th>
			<th> Qualité </th>
			<th> Livraison </th>
			<th> </th>
		</tr>
	</thead>
	
	<tbody>
		<% @prints.each do |print| %>
		<tr>
			<td><%= print.name %></td>
			<td><%= link_to "Download File", print.attachment_url %></td>
			<td><%= print.materiau %></td>
			<td><%= print.qualite %></td>
			<td><%= print.livraison%></td>
			<td><%= button_to "Delete", print, method: :delete, class: "btn 			btn-danger", confirm: "Supprimer l'impression ? L'action est 			irreversible."%>
		</tr>
		<% end %>
	</tbody>
</table>
			