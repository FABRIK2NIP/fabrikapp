<h1>Prints#new</h1>
<p>Find me in app/views/prints/new.html.erb</p>

<% if !@print.errors.empty? %>
	<div class="alert-error">
		<ul>
			<% @print.errors.full_messages.each do |msg|%>
			<li> <%= msg %></li>
			<% end %>
		</ul>
<% end %>

<div class="well">
		<%= form_for @print, html: { :multipart => true } do |f| %>
			<%= f.label :name %>
			<%= f.text_field :name %>
			<%= f.label :attachment %>
			<%= f.file_field :attachment %>
			
			<%= f.submit "save", class: "btn btn-primary"%>
		<% end %>
</div>
